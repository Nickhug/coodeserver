    "test": "turbo run test"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "turbo": "^1.11.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@10.2.3",
  "dependencies": {
    "@clerk/backend": "^1.31.4",
    "@google/generative-ai": "^0.24.1"
  }
}
